			<ref bean="h2"/>
			<ref bean="h4"/>
			<ref bean="h6"/>
		</list></constructor-arg>
	</bean>
	
	<bean id="royalFlushTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="hA"/>
			<ref bean="h10"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="hK"/>
			<ref bean="hJ"/>
			<ref bean="hD"/>
			<ref bean="s4"/>
			<ref bean="c6"/>
		</list></constructor-arg>
	</bean>
	
	<bean id="royalFlushTest2" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="hA"/>
			<ref bean="h10"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="hK"/>
			<ref bean="hJ"/>
			<ref bean="h4"/>
			<ref bean="s4"/>
			<ref bean="c6"/>
		</list></constructor-arg>
	</bean>
	
	<bean id="player1" class="ru.pokerbender.Player">
		<property name="cards">
			<list>	<ref bean="hA"/> <ref bean="h10"/>	</list>
		</property>
	</bean>
	
	<bean id="player2" class="ru.pokerbender.Player">
		<property name="cards">
			<list>	<ref bean="hA"/> <ref bean="cA"/>	</list>
		</property>
	</bean>
	
	<util:list id="river1" value-type="ru.pokerbender.Card">
    	<ref bean="hK"/>
		<ref bean="hJ"/>
		<ref bean="h4"/>
		<ref bean="s4"/>
		<ref bean="c10"/>
    </util:list>
    
    <util:list id="river2" value-type="ru.pokerbender.Card">
    	<ref bean="hK"/>
		<ref bean="cK"/>
		<ref bean="sK"/>
		<ref bean="s4"/>
		<ref bean="c4"/>
    </util:list>
    
    <util:list id="river3" value-type="ru.pokerbender.Card">
    	<ref bean="hK"/>
		<ref bean="cD"/>
		<ref bean="sJ"/>
		<ref bean="s10"/>
		<ref bean="dA"/>
