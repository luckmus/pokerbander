	<bean id="hK" class="ru.pokerbender.Card"><property name="suit" value="Hearts"/> 	<property name="weight" value="King"/></bean>
	<bean id="dK" class="ru.pokerbender.Card"><property name="suit" value="Diamonds"/> 	<property name="weight" value="King"/></bean>
	<bean id="cK" class="ru.pokerbender.Card"><property name="suit" value="Clubs"/> 	<property name="weight" value="King"/></bean>
	<bean id="sK" class="ru.pokerbender.Card"><property name="suit" value="Spades"/> 	<property name="weight" value="King"/></bean>
	
	
	<bean id="hA" class="ru.pokerbender.Card"><property name="suit" value="Hearts"/> 	<property name="weight" value="Ace"/></bean>
	<bean id="dA" class="ru.pokerbender.Card"><property name="suit" value="Diamonds"/> 	<property name="weight" value="Ace"/></bean>
	<bean id="cA" class="ru.pokerbender.Card"><property name="suit" value="Clubs"/> 	<property name="weight" value="Ace"/></bean>
	<bean id="sA" class="ru.pokerbender.Card"><property name="suit" value="Spades"/> 	<property name="weight" value="Ace"/></bean>
		
	<bean id="pairTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg>
			<list>
			<ref bean="h3"/>
			<ref bean="s1"/>
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
			<ref bean="d3"/>
			<ref bean="s2"/>
			<ref bean="c1"/>
			</list>
		</constructor-arg>
	</bean>	 
	
	<bean id="doublePairTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg>
			<list>
			<ref bean="h3"/>
			<ref bean="s1"/>
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
			<ref bean="d3"/>
			<ref bean="s2"/>
			<ref bean="c1"/>
			<ref bean="d4"/>
			<ref bean="h5"/>
			</list>
		</constructor-arg>
	</bean>	
	
	<bean id="fullHouseTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg>
			<list>
			<ref bean="h3"/>
			<ref bean="s1"/>
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
			<ref bean="d3"/>
			<ref bean="s2"/>
			<ref bean="c2"/>
			<ref bean="d2"/>
			<ref bean="h5"/>
			</list>
		</constructor-arg>
	</bean> 
	
	
	
	<bean id="kikkerTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg>
			<list>
			<ref bean="h3"/>
			<ref bean="s1"/>
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
			<ref bean="d3"/>
			<ref bean="s2"/>
			<ref bean="c2"/>
			<ref bean="d2"/>
			<ref bean="h4"/>
			</list>
		</constructor-arg>
	</bean>
	
	<bean id="setTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="h3"/>
			<ref bean="s1"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="d3"/>
			<ref bean="s4"/>
			<ref bean="c2"/>
			<ref bean="d2"/>
			<ref bean="h3"/>
		</list></constructor-arg>
	</bean>
	<bean id="streetTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="h3"/>
			<ref bean="s1"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="d3"/>
			<ref bean="s4"/>
			<ref bean="c2"/>
			<ref bean="d5"/>
			<ref bean="h3"/>
		</list></constructor-arg>
	</bean>
	
	<bean id="flushTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="h3"/>
			<ref bean="s1"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="d3"/>
			<ref bean="h4"/>
			<ref bean="h2"/>
			<ref bean="h5"/>
			<ref bean="h1"/>
		</list></constructor-arg>
	</bean>
	<bean id="flushTest2" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="c3"/>
			<ref bean="s1"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="d3"/>
			<ref bean="c4"/>
			<ref bean="c2"/>
			<ref bean="c5"/>
