		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="d3"/>
			<ref bean="h3"/>
			<ref bean="c2"/>
			<ref bean="s3"/>
			<ref bean="c1"/>
		</list></constructor-arg>
	</bean>
	
	<bean id="straightFlushTest1" class="ru.pokerbender.ResultCalculator">
		<constructor-arg><list>
			<ref bean="h3"/>
			<ref bean="h1"/>
		</list></constructor-arg>
		<constructor-arg><list>
			<ref bean="c3"/>
			<ref bean="s3"/>
			<ref bean="h2"/>
			<ref bean="h4"/>
			<ref bean="h5"/>
		</list></constructor-arg>
	</bean>
